    "@yext/search-headless-react": "^2.0.1",
    "@yext/search-ui-react": "^1.0.2",
    "react": "^17.0.2",
    "react-collapse": "^5.1.1",
    "react-collapsed": "^3.6.0",
    "react-dom": "^17.0.2",
    "react-phone-number-input": "^3.2.23",
    "react-router": "^6.8.2",
